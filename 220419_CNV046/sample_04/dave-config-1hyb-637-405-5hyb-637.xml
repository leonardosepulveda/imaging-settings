<?xml version="1.0" encoding="ISO-8859-1"?>
<recipe>
	<command_sequence>
		<wakeup>10000</wakeup>
		<loop name = "Movie Loop 1">
			<movie>
				<name increment = "Yes">hal-mf3</name>
				<length>90</length>
				<parameters>hal-config-637-405-dz02-z90</parameters>
				<check_focus>
                    <min_sum>60000</min_sum>
					<num_focus_checks>20</num_focus_checks>
					<focus_scan/>
				</check_focus>
				<overwrite>False</overwrite>
				<variable_entry name = "Movie Loop 1"></variable_entry>
			</movie>
		</loop>

		<loop name="Hybridize Loop 1">
			<variable_entry name = "Hybridize Loop 1"></variable_entry>
			<wakeup>10000</wakeup>
			<loop name = "Movie Loop 2">
				<movie>
					<name increment = "Yes">hal-mf3</name>
					<length>45</length>
					<parameters>hal-config-637-dz02-z45</parameters>
					<check_focus>
                        <min_sum>60000</min_sum>
						<num_focus_checks>20</num_focus_checks>
						<focus_scan/>
					</check_focus>
					<overwrite>False</overwrite>
					<variable_entry name = "Movie Loop 2"></variable_entry>
				</movie>
			</loop>
		</loop>
	</command_sequence>

	<loop_variable name = "Movie Loop 1">
		<file_path></file_path>
	</loop_variable>

	<loop_variable name = "Movie Loop 2">
		<file_path></file_path>
	</loop_variable>

	<loop_variable name = "Hybridize Loop 1">
		<value>
			<valve_protocol>Cleave Adaptors 18 min, Wash 5 min</valve_protocol>
			<valve_protocol>Flow Adaptor 2, 12 min</valve_protocol>
			<valve_protocol>Wash 5 min, Flow readouts 12 min, Wash 5 min, Flow imaging buffer 5 min</valve_protocol>
		</value>
		<value>
			<valve_protocol>Cleave Adaptors 18 min, Wash 5 min</valve_protocol>
			<valve_protocol>Flow Adaptor 3, 12 min</valve_protocol>
			<valve_protocol>Wash 5 min, Flow readouts 12 min, Wash 5 min, Flow imaging buffer 5 min</valve_protocol>
		</value>
		<value>
			<valve_protocol>Cleave Adaptors 18 min, Wash 5 min</valve_protocol>
			<valve_protocol>Flow Adaptor 4, 12 min</valve_protocol>
			<valve_protocol>Wash 5 min, Flow readouts 12 min, Wash 5 min, Flow imaging buffer 5 min</valve_protocol>
		</value>
		<value>
			<valve_protocol>Cleave Adaptors 18 min, Wash 5 min</valve_protocol>
			<valve_protocol>Flow Adaptor 5, 12 min</valve_protocol>
			<valve_protocol>Wash 5 min, Flow readouts 12 min, Wash 5 min, Flow imaging buffer 5 min</valve_protocol>
		</value>
		<value>
			<valve_protocol>Cleave Adaptors 18 min, Wash 5 min</valve_protocol>
			<valve_protocol>Flow Adaptor 6, 12 min</valve_protocol>
			<valve_protocol>Wash 5 min, Flow readouts 12 min, Wash 5 min, Flow imaging buffer 5 min</valve_protocol>
		</value>
		
	</loop_variable>
</recipe>
