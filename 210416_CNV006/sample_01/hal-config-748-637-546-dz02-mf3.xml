<?xml version="1.0" encoding="ISO-8859-1"?>
<settings>
  <!-- camera settings -->
  <camera1>
    <colortable type="string">ramp_sat.ctbl</colortable>
    <exposure_time type="float">0.1</exposure_time>
    <exposure_value type="float">0.1</exposure_value>
    <flip_horizontal type="int">False</flip_horizontal>
    <flip_vertical type="int">False</flip_vertical>
    <max_intensity type="int">65536</max_intensity>
    <transpose type="int">False</transpose>
    <scalemax type="int">2000</scalemax>
    <scalemin type="int">100</scalemin>
    <sync type="int">0</sync>
  </camera1>

  <!-- film settings -->
  <film>
    <acq_mode type="string">fixed_length</acq_mode>
    <auto_increment type="int">True</auto_increment>
    <auto_shutters type="int">True</auto_shutters>
    <filename type="string">hal</filename> 
    <filetype type="string">.dax</filetype>
    <frames type="int">125</frames>
    <logfile type="string">c:\Data\image_log.txt</logfile>
    <want_bell type="int">True</want_bell>
  </film>

  
  <!-- illumination settings -->
  <illumination>
  	<default_power type="custom">1.000,0.500,0.050,0.050,0.001</default_power>
  	<shutters type="string">shutter-config-748-637-546.xml</shutters>
    <on_off_state type="custom">False,False,False,False,False</on_off_state>
    <power_buttons type="custom">[[['Max', 1.0], ['Low', 0.1]], [['Max', 1.0], ['Low', 0.1]], [['Max', 1.0], ['Low', 0.1]], [['Max', 1.0], ['Low', 0.1]], [['Max', 1.0], ['Low', 0.0088]]]</power_buttons>
  </illumination>

  <!-- focus lock settings -->
  <focuslock>
    <!--
  	<hardware_z_scan>
      <z_offsets type="string">0, 0.2,0.2, 0.4,0.4, 0.6,0.6, 0.8,0.8, 1.0,1.0, 1.2,1.2, 1.4,1.4, 1.6,1.6, 1.8,1.8,  </z_offsets>
    </hardware_z_scan>
    -->

    <cal_deadtime type="int">10</cal_deadtime>
    <cal_frames_to_pause type="int">3</cal_frames_to_pause>
    <cal_range type="float">600.0</cal_range>
    <cal_step_size type="float">10.0</cal_step_size>
    <ir_power type="int">6</ir_power>
    <olock_bracket_step type="float">1000.0</olock_bracket_step>
    <olock_quality_threshold type="float">0.0</olock_quality_threshold>
    <olock_scan_hold type="int">10</olock_scan_hold>
    <olock_scan_step type="float">100.0</olock_scan_step>
    <qpd_scale type="float">50.0</qpd_scale>
    <qpd_zcenter type="float">150.0</qpd_zcenter>
    <zscan_focus_lock type="boolean">False</zscan_focus_lock>
    <zscan_frames_to_pause type="int">3</zscan_frames_to_pause>
    <zscan_start type="float">50.0</zscan_start>
    <zscan_step type="float">0.2</zscan_step>
    <zscan_stop type="float">3.5</zscan_stop>
  </focuslock>
  

</settings>
