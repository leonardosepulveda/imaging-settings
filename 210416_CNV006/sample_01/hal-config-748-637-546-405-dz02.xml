<?xml version="1.0" encoding="ISO-8859-1"?>
<settings>
  <!-- camera settings-->
  <camera1>

    <!-- actual Andor features -->
    <exposure_time type="float">0.2</exposure_time>

    <!-- view settings -->
    <flip_horizontal type="int">0</flip_horizontal>
    <flip_vertical type="int">0</flip_vertical>
    <transpose type="int">0</transpose>
    <scalemax type="int">4096</scalemax>
    <scalemin type="int">0</scalemin>
    <max_intensity type="int">65536</max_intensity>
    <colortable type="string">ramp_sat.ctbl</colortable>
    <sync type="int">0</sync>
  </camera1>

  <!-- film settings -->
  <film>
    <filename type="string">test</filename> 
    <acq_mode type="string">fixed_length</acq_mode>
    <frames type="int">32</frames>
    <logfile type="string">c:\Data\image_log.txt</logfile>
    <auto_increment type="int">1</auto_increment>
    <want_bell type="int">1</want_bell>
    <auto_shutters type="int">1</auto_shutters>
    <filetype type="string">.dax</filetype>
  </film>

  <!-- illumination settings -->
  <illumination>
    <default_power type="custom">1.000,0.500,0.050,0.050,0.001</default_power>
    <shutters type="string">shutter-config-748-637-546-405.xml</shutters>
    <on_off_state type="custom">False,False,False,False,False</on_off_state>
    <power_buttons type="custom">[[['Max', 1.0], ['Low', 0.1]], [['Max', 1.0], ['Low', 0.1]], [['Max', 1.0], ['Low', 0.1]], [['Max', 1.0], ['Low', 0.1]], [['Max', 1.0], ['Low', 0.001]]]</power_buttons>
  </illumination>


  <!-- focus lock settings -->
  <focuslock>
    <hardware_z_scan>
      <z_offsets type="string">0,0, 1.5,1.5,1.5,1.5,1.5, 3,3,3,3,3, 4.5,4.5,4.5,4.5,4.5, 6,6,6,6,6, 7.5,7.5,7.5,7.5,7.5, 9,9,9,9,9</z_offsets>
    </hardware_z_scan>
  </focuslock>

</settings>


